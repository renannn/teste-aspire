{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Aspire": {
    "Seq": {
      "DisableHealthChecks": true,
      "ServerUrl": "http://localhost:5341"
    }
  },
  "Keycloak": {
    "realm": "WeatherShop",
    "auth-server-url": "http://localhost:8080/",
    "ssl-required": "external",
    "resource": "WeatherWeb",
    "public-client": true,
    "confidential-port": 0
  }
}
